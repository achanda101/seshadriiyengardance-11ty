          <script>
            var logoImgMob = document.getElementsByClassName("logo-box")[0];
            var logoImgDesk = document.getElementsByClassName("logo-box")[1];
            var mobileElem = document.getElementsByClassName("mobile")[0];
            var navElem = document.getElementsByTagName("nav")[0];
            var navBarMobElem = document.getElementsByClassName("navbar")[0];
            var navBarDeskElem = document.getElementsByClassName("navbar")[1];
            var innerPage = document.getElementById("innerPage");
            var navBarMobInnerPage = document.getElementsByClassName("inner-page")[0];
            var hamMenuElem = document.getElementById("menu");
            var hamIcon = document.getElementById("ham-icon");
            var contentElem = document.getElementsByClassName("content")[0];
            var heroImage = document.getElementsByClassName("hero-image")[0];
            var heroSlider = document.getElementsByClassName("hero-slider")[0];
            var vanillaSlideshow = document.getElementById("vanilla-slideshow-container");
            const noHero = document.getElementById("noHero");

            // When HOMEPAGE has a banner video
            var heroVideo = document.getElementById("heroVideo");
            var audioToggleElem = document.getElementsByClassName("volume-icon")[0];

            var ht = window.innerHeight;
            var a_third_ht = ht * 0.33;

            // Global variables
            const navbar_bg_color_solid = "rgba(0, 0, 0, 1.0)";
            var navbar_bg_color_trans = "rgba(0, 0, 0, 0)";
            if (noHero) 
              navbar_bg_color_trans = "rgba(0, 0, 0, 1.0)";
            const navbar_bg_color_semitrans = "linear-gradient(to bottom, rgb(0, 0, 0, 0.6), rgba(0, 0, 0, 0))";
            const navbar_height = "100px";
            const navbar_height_onscroll = "70px";
            const navbar_desk_height = "120px";
            const navbar_desk_height_onscroll = "100px";

            window.scroll({behavior: "smooth"});

            window.addEventListener("scroll", () => {
              let fromTop = window.scrollY;

              if (fromTop > 0) {
                if (logoImgMob) {
                  logoImgMob.style.width = "40px";
                  logoImgMob.style.height = "40px";
                }
                if (logoImgDesk) {
                  logoImgDesk.style.width = "70px";
                  logoImgDesk.style.height = "70px";
                }
                navBarMobElem.style.height = navbar_height_onscroll;
                navBarMobElem.style.backgroundColor = navbar_bg_color_solid;
                navBarDeskElem.style.height = navbar_desk_height_onscroll;
                navBarDeskElem.style.backgroundColor = navbar_bg_color_solid;

                if (fromTop > a_third_ht) {
                  if (heroVideo) {
                    heroVideo.pause();
                  }
                }
              }

              if (fromTop < 1) {
                if (!hamMenuElem.checked) {
                  if (logoImgMob) {
                    logoImgMob.style.width = "clamp(70px, 6vw, 120px)";
                    logoImgMob.style.height = "clamp(70px, 6vw, 120px)";
                  }
                  if (logoImgDesk) {
                    logoImgDesk.style.width = "clamp(70px, 6vw, 120px)";
                    logoImgDesk.style.height = "clamp(70px, 6vw, 120px)";
                  }
                  navBarMobElem.style.height = navbar_height;
                  navBarMobElem.style.backgroundColor = navbar_bg_color_trans;
                  navBarDeskElem.style.height = navbar_desk_height;
                  navBarDeskElem.style.backgroundColor = navbar_bg_color_trans;
                  if (innerPage) {
                    navBarDeskElem.style.backgroundColor = navbar_bg_color_semitrans;
                    navBarMobInnerPage.style.backgroundColor = navbar_bg_color_semitrans;
                    navBarMobElem.style.backgroundColor = "transparent";
                  }

                  if (fromTop < a_third_ht) {
                    if (heroVideo) {
                      heroVideo.play();
                    }
                  }
                }
              }
            });

            hamMenuElem.addEventListener("click", () => {
              let fromTop = window.scrollY;
              if (hamMenuElem.checked) {
                mobileElem.style.backgroundColor = navbar_bg_color_solid;
                navBarMobElem.style.height = navbar_height;
                if (logoImgMob) {
                  logoImgMob.style.width = "40px";
                  logoImgMob.style.height = "40px";
                }
                mobileElem.style.maxHeight = "100vh";
                mobileElem.style.overflowY = "auto";
                document.body.style.overflowY = "hidden";
                if (heroVideo) {
                  heroVideo.pause();
                }
                hamIcon
                  .classList
                  .toggle("ham-open");
              } else {
                mobileElem.style.backgroundColor = navbar_bg_color_trans;
                if (innerPage) {
                  mobileElem.style.background = navbar_bg_color_semitrans;
                }
                if (noHero) {
                  mobileElem.style.backgroundColor = navbar_bg_color_solid;
                }
                if (logoImgMob) {
                  if (fromTop < 1) {
                    logoImgMob.style.width = "clamp(70px, 6vw, 120px)";
                    logoImgMob.style.height = "clamp(70px, 6vw, 120px)";
                  } else {
                    logoImgMob.style.width = "40px";
                    logoImgMob.style.height = "40px";
                  }
                }
                navBarMobElem.style.height = navbar_height;
                document.body.style.overflowY = "auto";
                if (heroVideo) {
                  heroVideo.play();
                }
                hamIcon
                  .classList
                  .toggle("ham-open");
              }
            });

            if (audioToggleElem) {
              audioToggleElem.addEventListener("click", () => {
                if (heroVideo && heroVideo.muted) {
                  audioToggleElem.innerHTML = "<i class='fas fa-volume-up'></i>";
                } else {
                  audioToggleElem.innerHTML = "<i class='fas fa-volume-mute'></i>";
                }
                heroVideo.muted = !heroVideo.muted;
              });
            }

            if (ht > 799) {
              if (heroImage) {
                heroImage.style.marginTop = "-" + navbar_desk_height;
              }

              if (heroSlider) {
                heroSlider.style.marginTop = "-" + navbar_desk_height;
              }
            }

            $(document).ready(function () {
              // prevent download of images through mouse events
              $("img").bind("contextmenu", function (e) {
                return false;
              });

              // If the banner is NOT a video or it is an INNERPAGE, display the menubar right away
              if (!heroVideo || innerPage) {
                if (navBarDeskElem) 
                  navBarDeskElem.style.visibility = "visible";
                }
              });

            // GSAP ScrollTrigger for vertical-fixed elements
            // and for scroll-to-the-top element
            gsap.registerPlugin(ScrollTrigger);
            const vert_fixed_elem_list = document.querySelectorAll(".vertical-fixed");
            if (vert_fixed_elem_list) {
              vert_fixed_elem_list.forEach(vf_elem => {
                gsap.to(vf_elem, {
                  display: "none",
                  scrollTrigger: {
                    trigger: "footer",
                    end: () => `-=${document
                      .querySelector("footer")
                      .offsetHeight}`,
                    toggleActions: "play reset play reset"
                  }
                })
              });
            }

            const scroll_top_elem = document.querySelector("#scroll-to-top");
            if (scroll_top_elem) {
              gsap.to(scroll_top_elem, {
                display: "block",
                scrollTrigger: {
                  trigger: "main",
                  toggleActions: "play reset play reset"
                }
              });
            }

            function scrollToTop() {
              //scroll to 0 pixels down from the top
              gsap.to(window, {
                duration: 1,
                scrollTo: 0
              });
            }
          </script>
        </body>
